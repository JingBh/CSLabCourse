<view class="page">
    <view class="weui-form">
        <view class="weui-form__text-area">
            <h2 class="weui-form__title">高研社团作业</h2>
        </view>
        <view class="weui-form__control-area">
            <view class="weui-cells__group weui-cells__group_form">
                <view class="weui-cells weui-cells_form">
                    <view class="weui-cell weui-cell_active">
                        <view class="weui-cell__hd"><label class="weui-label">输入框</label></view>
                        <view class="weui-cell__bd">
                            <input class="weui-input"
                                   placeholder="请输入一些文字"
                                   placeholder-class="weui-input__placeholder"
                                   bindinput="input" />
                        </view>
                    </view>
                </view>
            </view>
        </view>
        <view class="weui-form__opr-area">
            <a class="weui-btn weui-btn_primary" bindtap="print">确定</a>
        </view>
        <view class="weui-form__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__text">JingBh @ 2020-11-18</view>
            </view>
        </view>
    </view>
</view>
